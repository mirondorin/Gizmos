; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=4

_global_script_classes=[ {
"base": "Node",
"class": "BoardViewer",
"language": "GDScript",
"path": "res://Scripts/BoardViewer.gd"
}, {
"base": "TextureButton",
"class": "Card",
"language": "GDScript",
"path": "res://Scripts/Card.gd"
}, {
"base": "Node",
"class": "HintManager",
"language": "GDScript",
"path": "res://Scripts/HintManager.gd"
}, {
"base": "Node",
"class": "Player",
"language": "GDScript",
"path": "res://Scripts/Player.gd"
}, {
"base": "MarginContainer",
"class": "ScoreEntry",
"language": "GDScript",
"path": "res://Scripts/ScoreEntry.gd"
} ]
_global_script_class_icons={
"BoardViewer": "",
"Card": "",
"HintManager": "",
"Player": "",
"ScoreEntry": ""
}

[application]

config/name="Gizmos"
run/main_scene="res://Scenes/Lobby.tscn"

[autoload]

Utils="*res://Scripts/Utils.gd"
GameManager="*res://Scripts/GameManager.gd"
Server="*res://Scripts/Server.gd"

[display]

window/size/width=1920
window/size/height=1080
window/size/resizable=false
window/size/test_width=1600
window/size/test_height=900
window/stretch/mode="2d"
window/stretch/aspect="keep"

[rendering]

environment/default_environment="res://default_env.tres"
